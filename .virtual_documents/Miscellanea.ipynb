def eqnorm(A,b):
 
    G= A.T@A
    f=A.t@b
    
    L=sp.linalg.cholesky(G,lower=True)
    U=L.T
    y,flag =Lsolve(L,f)
    if flag == 0:
        x,flag=Usolve(U,y)
    return x










