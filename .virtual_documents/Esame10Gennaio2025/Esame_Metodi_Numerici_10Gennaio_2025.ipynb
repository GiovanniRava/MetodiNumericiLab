def plagr(xnodi,j):
    
    xzeri=np.zeros_like(xnodi)
    n=xnodi.size
    if j==0:
       xzeri=xnodi[1:n]
    else:
       xzeri=np.append(xnodi[1:j],xnodi[j+1:n])
    
    num = np.poly(xzeri)
    den = np.polyval(num,xnodi[j])
    
    p= num/den
    
    return p



def InterpL(x, y, xx):
     
     n=x.size
     m=xx.size 
     L=np.zeros((m,n))
     for j in range(n) :
        p=plagr(x,j) 
        L[:,j]=np.polyval(p,xx)
    
    
     return L@y 





